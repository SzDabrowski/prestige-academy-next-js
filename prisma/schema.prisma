generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model CourseClient {
  id         String   @id @default(auto()) @map("_id") @db.ObjectId
  courseName String
  name       String
  pairName   String?
  email      String
  phone      String   @default("000-000-000")
  createdAt  DateTime? @default(now())
}

model PreschoolClient {
  id            String   @id @default(auto()) @map("_id") @db.ObjectId
  preschoolName String
  groupName     String?
  studentName   String
  parentName    String
  email         String
  phone         String   @default("000-000-000")
  createdAt     DateTime? @default(now())
}